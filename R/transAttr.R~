transAttr <- function(first,second,...)
  UseMethod("transAttr")

transAttr.default <- function(first,second) {
}

transAttr.station <- function(first,second,...) {
  #print("transAttr.station")
  attr(first,'long name') -> attr(second,'long name')
  attr(first,'timeunit') -> attr(second,'timeunit')
  attr(first,'longitude') -> attr(second,'longitude')
  attr(first,'latitude') -> attr(second,'latitude')
  attr(first,'altitude') -> attr(second,'altitude')
  attr(first,'location') -> attr(second,'location')
  attr(first,'station_id') -> attr(second,'station_id')
  attr(first,'quality') -> attr(second,'quality')
  attr(first,'calendar') -> attr(second,'calendar')
  attr(first,'country') -> attr(second,'country')
  attr(first,'source') -> attr(second,'source')
  attr(first,'URL') -> attr(second,'URL')
  attr(first,'history') -> attr(second,'history')
  attr(first,'reference') -> attr(second,'reference')
  attr(first,'parameter') -> attr(second,'parameter')
  attr(first,'unit') -> attr(second,'unit')
  attr(first,'info') -> attr(second,'info')
  attr(first,'call') -> attr(second,'call')
  #str(second)
  return(second)
}

transAttr.eof <- function(first,second) {
  attr(first,'station_id') -> attr(second,'statio_id') 
  attr(first,'quality') -> attr(second,'quality')
  attr(first,'modes') -> attr(second,'modes') 
  attr(first,'mean') -> attr(second,'mean') 
  attr(first,'max.autocor') -> attr(second,'max.autocor')
  attr(first,'eigenvalues') -> attr(second,'eigenvalues')
  attr(first,'sum.eigenv') -> attr(second,'sum.eigenv') 
  attr(first,'tot.var') -> attr(second,'tot.var')
  attr(first,'longitude') -> attr(second,'longitude') 
  attr(first,'latitude') -> attr(second,'latitude') 
  attr(first,'altitude') -> attr(second,'altitude')
  attr(first,'dimensions') -> attr(second,'dimensions')
  attr(first,'frequency') -> attr(second,'frequency')
  attr(first,'timeunit') -> attr(second,'timeunit')
  attr(first,'calendar') -> attr(second,'calendar')
  attr(first,'country') -> attr(second,'country')
  attr(first,'location') -> attr(second,'location')
  attr(first,'source') -> attr(second,'source')
  attr(first,'URL') -> attr(second,'URL')
  attr(first,'history') -> attr(second,'history') 
  attr(first,'long name') -> attr(second,'long name')
  attr(first,'aspect') -> attr(second,'aspect')
  attr(first,'unit') -> attr(second,'unit')
  attr(first,'parameter') -> attr(second,'parameter')
  attr(first,'reference') -> attr(second,'reference')
  attr(first,'info') -> attr(second,'info')
  return(second)
}

transAttr.field <- function(first,second,...) {
  attr(first,'long name') -> attr(second,'long name')
  attr(first,'timeunit') -> attr(second,'timeunit')
  attr(first,'longitude') -> attr(second,'longitude')
  attr(first,'latitude') -> attr(second,'latitude')
  attr(first,'altitude') -> attr(second,'altitude')
  attr(first,'location') -> attr(second,'location')
  attr(first,'station_id') -> attr(second,'station_id')
  attr(first,'quality') -> attr(second,'quality')
  attr(first,'calendar') -> attr(second,'calendar')
  attr(first,'country') -> attr(second,'country')
  attr(first,'source') -> attr(second,'source')
  attr(first,'URL') -> attr(second,'URL')
  attr(first,'history') -> attr(second,'history')
  attr(first,'reference') -> attr(second,'reference')
  attr(first,'parameter') -> attr(second,'parameter')
  attr(first,'unit') -> attr(second,'unit')
  attr(first,'info') -> attr(second,'info')
  attr(first,'call') -> attr(second,'call')
  #str(second)
  return(second)
}
